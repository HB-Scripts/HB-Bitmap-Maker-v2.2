        ( edit.Value >= 0 ) || edit.Value := 0
        ( edit.Value <= 4 ) || edit.Value := 4
        This.Bitmap.Smoothing := edit.Value
    }
    SetInterpolationMode( edit , * ){
        ( edit.Value >= 0 ) || edit.Value := 0
        ( edit.Value <= 7 ) || edit.Value := 7
